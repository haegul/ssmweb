<%- include header.ejs %>

<div class="container" style="margin-top: 10%;">

    <div class="col-md-12">
        <div class="pull-left" style="padding-right: 30px;">
            <img src="/static/images/error.png" width="120" height="120" alt="error image" />
        </div>

        <h1><%= message %></h1>
        <h2>Code : <%= error.status %></h2>

        <div class="clearfix" style="padding-top: 20px;">
            <pre><%= error.stack %></pre>
        </div>
    </div>

</div>

